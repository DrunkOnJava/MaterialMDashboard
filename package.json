    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "esbuild": "0.24.0",
    "globals": "15.12.0",
    "tailwindcss": "3.4.16",
    "vite": "6.0.4",
    "typescript": "^5.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "vitest": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2",
    "husky": "^9.0.10",
    "@testing-library/react": "^14.2.1",
    "@testing-library/jest-dom": "^6.4.2"
