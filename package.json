    "@radix-ui/react-toast": "^1.1.5",
    "chart.js": "^4.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.1",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.453.0",
